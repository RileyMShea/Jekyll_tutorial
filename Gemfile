# gem "rails"
gem "jekyll"
gem 'github-pages'

group :jekyll_plugins do
  gem 'jekyll-sitemap' # help search engines index content
  gem 'jekyll-feed' # create an rss feed for my posts
  gem 'jekyll-seo-tag' # adds meta tags to help with seo
end

gem 'wdm', '~> 0.1.0' if Gem.win_platform?